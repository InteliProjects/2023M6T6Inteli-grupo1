        "/seducUser/": {
            "get": {
                "description": "Retorna todos os usuários da SEDUC cadastrados no sistema",
                "responses": {
                    "200": {
                        "description": "Usuários retornados com sucesso"
                    },
                    "400": {
                        "description": "Erro ao retornar usuários"
                    }
                }
            },
            "post": {
                "description": "Cadastra um novo usuário da SEDUC no sistema",
                "responses": {
                    "200": {
                        "description": "Usuário cadastrado com sucesso"
                    },
                    "400": {
                        "description": "Erro ao cadastrar usuário"
                    }
                },
                "requestBody": {
                    "description": "Corpo da requisição para cadastrar um novo usuário",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    }
                                },
                                "required": ["email"]
                            }
                        }
                    }
                }
            }
        },
